<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - 捉妖工具</title>
    <script src="~/Scripts/jquery-1.10.2.js"></script>

    @*2、bootstrap组件引用*@
    <script src="~/Content/bootstrap/bootstrap.js"></script>
    <link href="~/Content/bootstrap/bootstrap.css" rel="stylesheet" />
    <script src="~/js/site.js" asp-append-version="true"></script>
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/2.0.0/clipboard.min.js"></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-default">
            <div class="container">
                <div class="navbar-header">
                    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">捉妖工具</a>
                </div>
                    <div>
                        <ul class="nav navbar-nav">
                            <li class="nav-item">
                                <a class="dropdown" asp-area="" asp-controller="Home" asp-action="Index">已知妖灵</a>
                            </li>
                            <li class="nav-item">
                                <a class="dropdown" asp-area="" asp-controller="Home" asp-action="Search">妖灵搜索</a>
                            </li>
                        </ul>
                    </div>
                </div>
        </nav>
    </header>
    <div class="container">
        @*<partial name="_CookieConsentPartial" />*@
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2019 - 捉妖工具
        </div>
    </footer>



    @RenderSection("Scripts", required: false)
</body>
</html>
